{
    "_version": "1.12.0",
    "sap.app": {
        "id": "pe.idc.web.test.springmvc",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "pe.idc.web.test.springmvc.view.Main",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": [
                "sap.m"
            ]
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "pe.idc.web.test.springmvc.i18n.i18n"
                }
            },            
            "side": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/sideContent.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },        
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlId": "mainContents",
                "viewType": "XML",
                "controlAggregation": "pages",
                "viewPath": "pe.idc.web.test.springmvc.view",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home",
                    "target": "home"
                },
                {
                    "pattern": "SalesOrders",
                    "name": "salesOrders",
                    "target": "salesOrders"
                }
            ],
            "targets": {
                "home": {
                    "viewId": "home",
                    "viewName": "Home"
                },
                "salesOrders": {
                    "viewId": "salesOrders",
                    "viewName": "SalesOrders"
                }
            }
        }
    } 
}